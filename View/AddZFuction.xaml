<Window x:Class="ЧисленныМетоды.AddZFuction"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ЧисленныМетоды"
        mc:Ignorable="d"
        Height="250" Width="500" ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen" Deactivated="Window_Deactivated" WindowStyle="ToolWindow"
        
        >
    <Border BorderBrush="Black" BorderThickness="1">
        <StackPanel >
            <Label Content="Коэффициенты целевой функции"/>
            <DockPanel Height="170" Width="500" x:Name="DockPanelZFuction">
                <Button DockPanel.Dock="Bottom" Content="Закрыть" Click="CloseButton"/>
                <Button DockPanel.Dock="Bottom" 
                        Content="Сохранить" 
                        Click="ButtonBase_OnClick"
                        VerticalAlignment="Bottom"/>
                
            </DockPanel>
            
            
        </StackPanel>
    </Border>
</Window>
